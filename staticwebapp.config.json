{
  "routes": [
    {
      "route": "/guests",
      "rewrite": "https://andres-y-marcela.azurewebsites.net/api/v1/guests",
      "allowedRoles": ["anonymous"],
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "route": "/api/*",
      "rewrite": "https://andres-y-marcela.azurewebsites.net/api/v1/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}